# YouTube Data API v3 Key
# Get your API key from: https://console.developers.google.com/
# Enable YouTube Data API v3 for your project
NEXT_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here

# Optional: Set quota limits
NEXT_PUBLIC_YOUTUBE_QUOTA_LIMIT=10000

# Environment Configuration
# Set to 'true' to force production mode
NEXT_PUBLIC_PRODUCTION_MODE=false

# Set to 'true' to force development mode (overrides production)
NEXT_PUBLIC_FORCE_DEVELOPMENT=false

# Set to 'true' to use mock APIs even in production
NEXT_PUBLIC_MOCK_APIS=false

# Socket.io server URL for production
NEXT_PUBLIC_SOCKET_URL=

# Base URL for the application
NEXT_PUBLIC_BASE_URL=

# Supabase Configuration (if using)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_CHAT=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=false

# Debug Settings
NEXT_PUBLIC_VERBOSE_LOGGING=false
NEXT_PUBLIC_SHOW_MOCK_INDICATORS=true