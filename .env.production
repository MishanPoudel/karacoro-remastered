# Production Environment Variables

# YouTube Data API v3 Key (REQUIRED)
NEXT_PUBLIC_YOUTUBE_API_KEY=your_production_youtube_api_key

# Production Mode
NODE_ENV=production
NEXT_PUBLIC_PRODUCTION_MODE=true

# Socket.io Server URL (REQUIRED - replace with your actual server URL)
NEXT_PUBLIC_SOCKET_URL=wss://your-socket-server.com

# Base URL (REQUIRED - replace with your actual domain)
NEXT_PUBLIC_BASE_URL=https://your-domain.com

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_CHAT=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=false

# Debug Settings (disable in production)
NEXT_PUBLIC_VERBOSE_LOGGING=false
NEXT_PUBLIC_SHOW_MOCK_INDICATORS=false